<%- include('../partials/header') %>
<div class="container">
    
    <h2>Browse All cat-ALE-logs</h2>

    <a href="/catalelogs/new">
        <button class="btn waves-effect waves-light">Add a New cat-ALE-log</button>
    </a>

    <ul class="collection with-header">
        <li class="collection-header"><h4>Name</h4></li>
        <% catalelogs.forEach(function(catalelog) { %>
          <li class="collection-item flow-text">
            <i class="material-icons prefix">local_bar</i>
            <a class="black-text" href="/catalelogs/<%= catalelog.id %>">
              <%= catalelog.name %>
            </a>
            <form action="/catalelogs/<%= catalelog.id %>?_method=DELETE" method="POST" class="secondary-content">
              <button type="submit"><i class="material-icons">delete</i></button>
            </form>
          </li>
        <% }); %>
      </ul>
</div>
<%- include('../partials/footer') %>