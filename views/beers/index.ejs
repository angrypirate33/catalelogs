<%- include('../partials/header') %>

<div class="container">
    <h2 class="indigo-text text-darken-4">Search Results</h2>

    <div class="row">
        <% beers.forEach(function(beer) { %>
            <div class="col s12 m8 l6 xl3 ">
                <h6>
                    <a href="/beers/<%= beer.id %>"><%= beer.name %></a>
                </h6>
            </div>
        <% }); %>
    </div>
</div>
<%- include('../partials/footer') %>